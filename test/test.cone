// A test program in Cone that does nothing interesting

mut glowy = 34u32;
mut glo2 i32 = 7;

fn cone() u32 {
    23; // This comment (and line) is ignored
    '\t';
    2.3e1;
    add(1); // Forward reference call + default parameter
    glowy = 3u32;
