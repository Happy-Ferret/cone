// A test program in Cone that does nothing interesting

mut glowy = 34u32;
mut glo2 i32 = 7;

fn cone() u32 {
    23; // This comment (and line) is ignored
    '\t';
    2.3e1;
    add(1, 2); // Forward reference call
    glowy = 3u32;
    glo2 = glowy; // cast
    glowy; // Implicit return of global var's value
}

fn add(a i32, mut b i32) i32 {
    b = 3; // Parm is mutable
    a;
}
