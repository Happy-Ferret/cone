// A test program in Cone that does nothing interesting

mut glowy = 34u32;
mut glo2 i32 = 7;

fn cone() u32 {
    mut unsy = 23;
    '\t';
    2.3e1;
    glowy = unsy;
    glo2 = glowy; // cast
